#compdef _aws_finder aws_finder


function _aws_finder {
  local -a commands

  _arguments -C \
    "1: :->cmnds" \
    "*::arg:->args"

  case $state in
  cmnds)
    commands=(
      "cloudfront:Find CloudFront distributions by domain"
      "completion:Generates shell completion scripts"
      "help:Help about any command"
      "instance:Find an instance by type, AMI or ip address"
      "log_group:Find a CloudWatch log group by name"
      "log_stream:Find a CloudWatch log stream by name"
      "s3_bucket:Find an S3 bucket by name"
      "tag:Find resources by tags"
      "vpc:Find a VPC with the given CIDR range"
      "vpc_endpoint:Find a VPC endpoint by the given service name"
      "vpc_endpoint_service:Find a VPC endpoint service by the given service name"
    )
    _describe "command" commands
    ;;
  esac

  case "$words[1]" in
  cloudfront)
    _aws_finder_cloudfront
    ;;
  completion)
    _aws_finder_completion
    ;;
  help)
    _aws_finder_help
    ;;
  instance)
    _aws_finder_instance
    ;;
  log_group)
    _aws_finder_log_group
    ;;
  log_stream)
    _aws_finder_log_stream
    ;;
  s3_bucket)
    _aws_finder_s3_bucket
    ;;
  tag)
    _aws_finder_tag
    ;;
  vpc)
    _aws_finder_vpc
    ;;
  vpc_endpoint)
    _aws_finder_vpc_endpoint
    ;;
  vpc_endpoint_service)
    _aws_finder_vpc_endpoint_service
    ;;
  esac
}

function _aws_finder_cloudfront {
  _arguments
}

function _aws_finder_completion {
  _arguments \
    '(-h --help)'{-h,--help}'[help for completion]' \
    '1: :("bash" "zsh")'
}

function _aws_finder_help {
  _arguments
}

function _aws_finder_instance {
  _arguments
}

function _aws_finder_log_group {
  _arguments
}

function _aws_finder_log_stream {
  _arguments
}

function _aws_finder_s3_bucket {
  _arguments
}

function _aws_finder_tag {
  _arguments
}

function _aws_finder_vpc {
  _arguments
}

function _aws_finder_vpc_endpoint {
  _arguments
}

function _aws_finder_vpc_endpoint_service {
  _arguments
}
