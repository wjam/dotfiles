[Desktop Entry]
Name=WezTerm
Comment=Wez's Terminal Emulator
Keywords=shell;prompt;command;commandline;cmd;
Icon=org.wezfurlong.wezterm
StartupWMClass=org.wezfurlong.wezterm
TryExec={{ joinPath (output "brew" "--prefix" | trim) "bin" "wezterm" }}
Exec={{ joinPath (output "brew" "--prefix" | trim) "bin" "wezterm" }}
Type=Application
Categories=System;TerminalEmulator;Utility;
Terminal=false
X-AppImage-Version={{ output (joinPath (output "brew" "--prefix" | trim) "bin" "wezterm") "--version" | trimPrefix "wezterm" | trim }}
